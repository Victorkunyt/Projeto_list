generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url  = env("DATABASE_URL")
}


model User {
id            String        @id @default(auto()) @map("_id") @db.ObjectId
name String
holderid String
cellphone String 
email String
gender String
password String
status Boolean

@@map("Users")

}